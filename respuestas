24 

1.9.3p194 :001 > a = 4
 => 4 
1.9.3p194 :002 > b = 2
 => 2 
1.9.3p194 :003 > c = <<HERE
1.9.3p194 :004"> --#{a}--
1.9.3p194 :005"> --#{b}--
1.9.3p194 :006"> HERE
 => "--4--\n--2--\n"


25

1.9.3p194 :007 > c = <<'HERE'
1.9.3p194 :008'> --#{a}--
1.9.3p194 :009'> --#{b}--
1.9.3p194 :010'> HERE
 => "--\#{a}--\n--\#{b}--\n"

