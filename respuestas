1.9.3p194 :019 > e = '.'*3
 => "..." 


29

1.9.3p194 :020 > a = 1
 => 1 
1.9.3p194 :021 > "#{a=a+1} "*3
 => "2 2 2 "

30

1.9.3p194 :022 > %w[this is a test]
 => ["this", "is", "a", "test"]

31

1.9.3p194 :023 > %w[\t \n]
 => ["\\t", "\\n"] 

32

1.9.3p194 :024 > %W[\t \n]
 => ["\t", "\n"]


33
1.9.3p194 :025 > nils = Array.new(3)
 => [nil, nil, nil]

34
1.9.3p194 :026 > zeros = Array.new(3, 0)
 => [0, 0, 0] 

35
